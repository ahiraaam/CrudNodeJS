<h1>Lista de Productos</h1>
<button onclick="window.location.href='./products/create'">
    Agregar Producto
</button>
{{#each products}}
<div class="card" onclick="window.location.href='./products/{{id}}'">
    <p>
        {{id}} - {{name}}
    </p>
</div>


{{/each}}
<section class="pagination ">
    {{#if hasPreviousPage}}
    <a href="/?page={{previousPage}}">{{previousPage}}</a>
    {{/if}}
    <a href="/?page={{currentPage}}">{{currentPage}}</a>
    {{#if hasNextPage}}
    <a href="/?page={{nextPage}}">{{nextPage}}</a>
    {{/if}}


</section>